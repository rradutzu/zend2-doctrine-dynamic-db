{
    "name": "rradutzu/zend2-doctrine-dynamic-db",
    "description": "Zend2/3 Doctrine Module that allows defining connections using dynamic database names",
    "version": "2.0.0",
    "license": "GPL-2.0",
    "require": {
        "php": "^8.2",
        "composer/installers": "~1.0",
	    "doctrine/doctrine-orm-module": "^6.0"
    },
    "autoload": {
        "psr-0": {
            "DoctrineDynamicDb\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true
        }
    }
}
